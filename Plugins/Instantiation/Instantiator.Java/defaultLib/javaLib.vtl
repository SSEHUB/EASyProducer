template javaLib() {

    /**
    * Emit the code of the Java code elements as given. 
    */
    def emitJava(sequenceOf(JavaCodeElement) elements) {
        for (JavaCodeElement e : elements) {
            '${e.toCode()}' !<CR>;
        }
    }
    
    /**
    * Emit the code of the Java code element as given. 
    */
    def emitJava(JavaCodeElement elt) {
        '${elt.toCode()}' | -1;
    }

}