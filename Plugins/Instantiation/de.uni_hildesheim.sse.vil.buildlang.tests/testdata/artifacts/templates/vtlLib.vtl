template vtlLib(Configuration config, FileArtifact target) {

    compound VtlTestType {
        String value;
        Integer count = 0;
        Boolean test = false;
    }
    
    def Boolean doSomething() {
        true;
    }
    
    def String masterTable() {
        "master";
    }
    
    def String asName(String s) {
        s;
    }

    def addTestType(setOf(VtlTestType) tests, String value, Integer count = 0, Boolean test = false) {
        VtlTestType tt = new VtlTestType();
        tt.value = value;
        tt.count = count;
        tt.test = test;
        tests.add(tt);
    }

}