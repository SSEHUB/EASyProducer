<?xml version="1.0" encoding="UTF-8"?>
<project name="jlxd-boot" default="maven" basedir=".">
	<!-- import of the global SSE Jenkins properties -->
	<property file="${user.home}/global-build.properties"/>
	<property file="${user.home}/qualimaster-build.properties"/>
	<include file="${user.home}/macros.xml"/>
	
    <!-- deploy to SSE Mvn repo -->
	<target name="maven"> 
		<mvnDeployResolveArtifact folderName="jlxd-boot" pomFile="pom.xml" jarFolder="target" artifactPattern="jlxd-boot-*.*.*.jar" />
	</target>
</project>
